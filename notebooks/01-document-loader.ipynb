      "source": [
        "from langchain.document_loaders import BSHTMLLoader\n",
        "\n",
        "documents = []\n",
        "\n",
        "for file in os.listdir(\"../html\"):\n",
        "    loader = BSHTMLLoader(os.path.join(\"../html\", file))\n",
        "    data = loader.load()\n",
        "    documents.append(data)\n"
      ]
    }
